          { "text": "Bukan tentang ketakutan.", "style": { "fontSize": 28, "fontWeight": 700, "align": "right", "color": "#222831" } },
          { "text": "Tapi tentang keberanian menjelajahi yang tak diketahui. Setiap penemuan dimulai dengan pertanyaan berani.", "style": { "fontSize": 22, "fontWeight": 400, "align": "right", "color": "#222831" } }
